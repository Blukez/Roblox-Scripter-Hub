-- Scripts:

local function AJWFXXP_fake_script() -- GameTitle.GameDetectScript 
	local script = Instance.new('LocalScript', GameTitle)

	if game.PlaceId == 7491533387 then
		script.Parent.Text = "Test Game"
	end

	if game.PlaceId == 286090429 then
		script.Parent.Text = "Arsenal"
	end
end
coroutine.wrap(AJWFXXP_fake_script)()
