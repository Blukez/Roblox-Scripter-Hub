-- Instances:

local RobloxScripterHubLauncher = Instance.new("ScreenGui")
local Main = Instance.new("Frame")
local Title = Instance.new("TextLabel")
local ImageLabel = Instance.new("ImageLabel")
local Gamee = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local Load = Instance.new("TextButton")
local UICorner_2 = Instance.new("UICorner")
local GameTitle = Instance.new("TextLabel")
local UICorner_3 = Instance.new("UICorner")

--Properties:

RobloxScripterHubLauncher.Name = "RobloxScripterHubLauncher"
RobloxScripterHubLauncher.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
RobloxScripterHubLauncher.Enabled = false
RobloxScripterHubLauncher.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Main.Name = "Main"
Main.Parent = RobloxScripterHubLauncher
Main.BackgroundColor3 = Color3.fromRGB(46, 46, 46)
Main.Position = UDim2.new(0.350454569, 0, 0.384982109, 0)
Main.Size = UDim2.new(0, 575, 0, 192)

Title.Name = "Title"
Title.Parent = Main
Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Title.BackgroundTransparency = 1.000
Title.Position = UDim2.new(0.0976157337, 0, 0.0939972401, 0)
Title.Size = UDim2.new(0, 305, 0, 38)
Title.Font = Enum.Font.SourceSans
Title.Text = "Roblox Scripter Hub"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.TextScaled = true
Title.TextSize = 14.000
Title.TextWrapped = true

ImageLabel.Parent = Main
ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ImageLabel.BackgroundTransparency = 1.000
ImageLabel.Position = UDim2.new(0.0205404032, 0, 0.0417427272, 0)
ImageLabel.Size = UDim2.new(0, 60, 0, 60)
ImageLabel.Image = "http://www.roblox.com/asset/?id=7553453866"

Gamee.Name = "Gamee"
Gamee.Parent = Main
Gamee.BackgroundColor3 = Color3.fromRGB(53, 53, 53)
Gamee.Position = UDim2.new(0.0313043483, 0, 0.408215851, 0)
Gamee.Size = UDim2.new(0, 539, 0, 95)

UICorner.Parent = Gamee

Load.Name = "Load"
Load.Parent = Gamee
Load.BackgroundColor3 = Color3.fromRGB(46, 46, 46)
Load.Position = UDim2.new(0.0189999994, 0, 0.545000017, 0)
Load.Size = UDim2.new(0, 519, 0, 31)
Load.Font = Enum.Font.SourceSans
Load.Text = "Load"
Load.TextColor3 = Color3.fromRGB(255, 255, 255)
Load.TextScaled = true
Load.TextSize = 14.000
Load.TextWrapped = true

UICorner_2.Parent = Load

GameTitle.Name = "GameTitle"
GameTitle.Parent = Gamee
GameTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
GameTitle.BackgroundTransparency = 1.000
GameTitle.Position = UDim2.new(0.257884979, 0, 0.0664184541, 0)
GameTitle.Size = UDim2.new(0, 261, 0, 38)
GameTitle.Font = Enum.Font.SourceSans
GameTitle.Text = "N/A"
GameTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
GameTitle.TextScaled = true
GameTitle.TextSize = 14.000
GameTitle.TextWrapped = true

UICorner_3.CornerRadius = UDim.new(0, 16)
