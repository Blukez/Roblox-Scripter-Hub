-- Scripts:

local function ADGI_fake_script() -- GameDetect.GameDetectScript 
	local script = Instance.new('LocalScript', GameDetect)

	if game.PlaceId == 7491533387 then
		script.Parent.Text = "Test Game"
	end
	
end
coroutine.wrap(ADGI_fake_script)()
local function TSCXCAG_fake_script() -- Main.AnimationManager 
	local script = Instance.new('LocalScript', Main)

	wait(2)
	
	local MAIN = script.Parent
	local GAMESECTION = script.Parent.Game
	
